{
	"manifest_version": 2,

	"name": "Keylogger",
	"description": "This extension records the keystrokes of all website",
	"version": "1.0",

	"browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html",
	"default_title": "Click Here"
	},
	"permissions": [
	"activeTab",
	"storage"
	]
}